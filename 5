#include <iostream>
#include <iomanip> 
using namespace std;
int main() {
    int n, k;
        
            // Ввод первого дня недели и количества дней в месяце
                cin >> n >> k;
                    // Печатаем пробелы перед первым днем
                        for (int i = 1; i < n; i++) {
                                cout << "   "; // 3 пробела для выравнивания
                                    }
                                        // Печать дней месяца
                                            for (int day = 1; day <= k; day++) {
                                                    cout << setw(2) << day; // Печатаем день с выравниванием (2 символа)
                                                            // Печатаем перевод строки, если день является последним в неделе
                                                                    if ((day + n - 1) % 7 == 0) {
                                                                                cout << endl;
                                                                                        } else {
                                                                                                    cout << " "; // Пробел между днями
                                                                                                            }
                                                                                                                }
                                                                                                                    cout << endl; // Завершаем вывод одним переводом строки
                                                                                                                        return 0;
                                                                                                                        }
