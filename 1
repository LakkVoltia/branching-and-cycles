#include <iostream>
#include <cmath> // для функции abs

using namespace std;

int main() {
    int x1, y1, x2, y2;
        
            // Ввод координат первой и второй клеток
                cin >> x1 >> y1 >> x2 >> y2;
                    
                        // Проверка условий для хода ферзя
                            if (x1 == x2||y1 == y2||  abs(x1 - x2) == abs(y1 - y2)) {
                                    cout << "YES" << endl;
                                        } else {
                                                cout << "NO" << endl;
                                                 }
                                                        
                                                            return 0;
                                                            }
